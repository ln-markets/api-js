{
  "name": "@ln-markets/api",
  "version": "3.0.0-0",
  "description": "A set of wrappers to easily communicate with LN Markets API !",
  "author": "Victor Afanassieff <victo@afanassieff.com>",
  "license": "MIT",
  "keywords": [
    "ln-markets",
    "api",
    "trading",
    "sats",
    "bitcoin"
  ],
  "bugs": {
    "url": "https://github.com/ln-markets/api-js/issues"
  },
  "homepage": "https://github.com/ln-markets/api-js/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ln-markets/api-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.6"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint --color './**/*.{ts,js}'",
    "format": "prettier --write './**/*.{ts,js}'",
    "build": "tsc --build",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "tsc",
    "release": "np --no-cleanup --no-tests --no-yarn"
  },
  "dependencies": {
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@types/node": "18.16.0",
    "@types/ws": "8.5.5",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "vitest": "0.32.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
